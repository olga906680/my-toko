					<div class="col-sm-10">
					<select name="tmdqc_country_id" id="input-country" class="form-control">
					{% for country in countries %}
					{% if country.country_id == tmdqc_country_id %}
					<option value="{{ country.country_id }}" selected="selected">{{ country.name }}</option>
					{% else %}
					<option value="{{ country.country_id }}">{{ country.name }}</option>
					{% endif %}
					{% endfor %}
					</select>
					</div>
					</div>
					<div class="form-group">
					<label class="col-sm-2 control-label" for="input-zone">{{ entry_zone }} <span data-toggle="tooltip"  data-original-title="{{ help_zone_default }}"></span></label>
					<div class="col-sm-10">
					<select name="tmdqc_zone_id" id="input-zone" class="form-control">
					</select>
					</div>
					</div>
					<div class="form-group">
					<label class="col-sm-2 control-label" for="input-account-option">{{ entry_account_option }} <span data-toggle="tooltip"  data-original-title="{{ help_account_option }}"></span></label>
					<div class="col-sm-10">
					<select name="tmdqc_account_option" id="input-register-guest" class="form-control">
					<option value="">{{ text_all }}</option>
					<option value="register" {{ tmdqc_account_option=='register' ? 'selected="selected"' }}>{{ text_register }}</option>
					<option value="guest" {{ tmdqc_account_option=='guest' ? 'selected="selected"' }}>{{ text_guest }}</option>
					</select>
					</div>
					</div>
					<div class="form-group">
                  <label class="col-sm-2 control-label" for="input-account"><span data-toggle="tooltip" title="{{ help_account }}">{{ entry_account }}</span></label>
                  <div class="col-sm-4">
                    <select name="tmdqc_account_id" id="input-account" class="form-control">
                      <option value="0">{{ text_none }}</option>
					{% for information in informations %}
					{% if  information.information_id == tmdqc_account_id %}
                      <option value="{{ information.information_id }}" selected="selected">{{ information.title }}</option>
					{% else %}
                      <option value="{{ information.information_id }}">{{ information.title }}</option>
                     {% endif %}
                    {% endfor %}
                    </select>
                  </div>
				<label class="col-sm-2 control-label" for="input-account-auto-select">{{ entry_default_select }}</label>
				<div class="col-sm-4">
				{% if tmdqc_account_default_checked %}
					<input type="radio" name="tmdqc_account_default_checked" value="1" data-radio-all-off="true" class="switch-radio"  checked="checked"/>
					{% else %}
					<input type="radio" name="tmdqc_account_default_checked" value="1" data-radio-all-off="true" class="switch-radio"  />
				{% endif %}			
				</div>
                </div>
				<div class="form-group">
                  <label class="col-sm-2 control-label" for="input-checkout"><span data-toggle="tooltip" title="{{ help_chkout }}">{{ entry_checkout }}</span></label>
                  <div class="col-sm-4">
                    <select name="tmdqc_checkout_id" id="input-checkout" class="form-control">
                      <option value="0">{{ text_none }}</option>
                      {% for information in informations %}
                      {% if  information.information_id == tmdqc_checkout_id %}
                      <option value="{{ information.information_id }}" selected="selected">{{ information.title }}</option>
                      {% else %}
                      <option value="{{ information.information_id }}">{{ information.title }}</option>
                      {% endif %}
                      {% endfor %}
                    </select>
                  </div>
					<label class="col-sm-2 control-label" for="input-checkout-auto-select">{{ entry_default_select }}</label>
					<div class="col-sm-4">
					{% if tmdqc_checkout_default_checked %}
					<input type="radio" name="tmdqc_checkout_default_checked" value="1" data-radio-all-off="true" class="switch-radio"  checked="checked"/>
					{% else %}
					<input type="radio" name="tmdqc_checkout_default_checked" value="1" data-radio-all-off="true" class="switch-radio"  />
					{% endif %}
					
					</div>
                </div>
				<div class="form-group">
                  <label class="col-sm-2 control-label" for="input-customer-group">{{ entry_customer_group }}</label>
                  <div class="col-sm-10">
                    <select name="tmdqc_customer_group_id" id="input-customer-group" class="form-control">
					{% for customer_group in customer_groups %}
					{% if customer_group.customer_group_id == tmdqc_customer_group_id %}
                      <option value="{{ customer_group.customer_group_id }}" selected="selected">{{ customer_group.name }}</option>
                     {% else %}
                      <option value="{{ customer_group.customer_group_id }}">{{ customer_group.name }}</option>
                      {% endif %}
                      {% endfor %}
                    </select>
                  </div>
                </div>
				</div>
	<div class="tab-pane" id="tab-cart">
